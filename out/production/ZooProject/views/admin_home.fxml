<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<VBox prefHeight="645.0" prefWidth="770.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controllers.AdminHomeController">
	<children>
		<AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
			<children>
				<Label fx:id="lblWelcome" alignment="CENTER" contentDisplay="CENTER" layoutX="-2.0" layoutY="48.0"
				       prefHeight="17.0" prefWidth="770.0" text="Welcome NAME" textAlignment="CENTER"
				       textOverrun="WORD_ELLIPSIS">
					<font>
						<Font size="36.0"/>
					</font>
				</Label>
				<Button layoutX="251.0" layoutY="323.0" mnemonicParsing="false" onAction="#registration"
				        text="Register Zoo Keeper">
					<font>
						<Font size="24.0"/>
					</font>
				</Button>
				<Button layoutX="294.0" layoutY="408.0" mnemonicParsing="false" onAction="#animalMenu"
				        text="Animal Menu">
					<font>
						<Font size="24.0"/>
					</font>
				</Button>
				<Button layoutX="326.0" layoutY="549.0" mnemonicParsing="false" onAction="#logout" text="Log out">
					<font>
						<Font size="24.0"/>
					</font>
				</Button>
				<TextArea fx:id="txtUserInfo" editable="false" layoutX="225.0" layoutY="113.0" prefHeight="185.0"
				          prefWidth="317.0" text="&#10;" wrapText="true">
					<font>
						<Font size="20.0"/>
					</font>
				</TextArea>
			</children>
		</AnchorPane>
	</children>
</VBox>
