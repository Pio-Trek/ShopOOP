<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<VBox prefHeight="645.0" prefWidth="770.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controllers.CustomerHomeController">
	<children>
		<AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
			<children>
				<Label fx:id="labelWelcome" alignment="CENTER" contentDisplay="CENTER" layoutX="-2.0" layoutY="48.0"
				       prefHeight="17.0" prefWidth="770.0" text="Welcome NAME" textAlignment="CENTER">
					<font>
						<Font size="36.0"/>
					</font>
				</Label>
				<Button layoutX="291.0" layoutY="439.0" mnemonicParsing="false" text="Close Account" textFill="RED"
				        onAction="#deleteCustomer">
					<font>
						<Font size="24.0"/>
					</font>
				</Button>
				<Button layoutX="326.0" layoutY="549.0" mnemonicParsing="false" onAction="#logout" text="Log out">
					<font>
						<Font size="24.0"/>
					</font>
				</Button>
				<Label alignment="CENTER" contentDisplay="CENTER" layoutY="101.0" prefHeight="35.0" prefWidth="770.0"
				       text="Enjoy Shopping!" textAlignment="CENTER">
					<font>
						<Font size="24.0"/>
					</font>
				</Label>
				<Button layoutX="311.0" layoutY="332.0" mnemonicParsing="false" text="Edit Details"
				        onAction="#editCustomer">
					<font>
						<Font size="24.0"/>
					</font>
				</Button>
				<Button layoutX="306.0" layoutY="260.0" mnemonicParsing="false" text="View Orders" onAction="#viewOrders">
					<font>
						<Font size="24.0"/>
					</font>
				</Button>
				<Button layoutX="294.0" layoutY="190.0" mnemonicParsing="false" text="View Products" onAction="#viewProducs">
					<font>
						<Font size="24.0"/>
					</font>
				</Button>
			</children>
		</AnchorPane>
	</children>
</VBox>
